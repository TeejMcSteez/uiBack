generator client {
  provider = "prisma-client-js"
  output   = "./prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model logs {
  id        Int      @id @default(autoincrement())
  timestamp DateTime @db.Timestamp(6)
  worker    String?
  component String?
  log       String
}

model news {
  id      Int     @id @default(autoincrement())
  creator String
  content String?
  guid    String?
}
